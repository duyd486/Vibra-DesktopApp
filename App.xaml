<Application x:Class="Vibra_DesktopApp.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Vibra_DesktopApp"
             xmlns:vm="clr-namespace:Vibra_DesktopApp.ViewModels"
             xmlns:views="clr-namespace:Vibra_DesktopApp.Views">
    <Application.Resources>
        <DataTemplate DataType="{x:Type vm:LoginViewModel}">
            <views:LoginView />
        </DataTemplate>
        <DataTemplate DataType="{x:Type vm:SignUpViewModel}">
            <views:SignUpView />
        </DataTemplate>
    </Application.Resources>
</Application>
