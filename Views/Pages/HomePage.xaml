<UserControl x:Class="Vibra_DesktopApp.Views.Pages.HomePage"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Vibra_DesktopApp.Views.Pages"
             xmlns:vm="clr-namespace:Vibra_DesktopApp.ViewModels"
             mc:Ignorable="d" 
             d:DesignHeight="750" d:DesignWidth="1250">
    <UserControl.DataContext>
        <vm:HomeViewModel/>
    </UserControl.DataContext>
    <Grid Background="Transparent">
        <Border CornerRadius="20" Background="Black" Margin="0 0 5 5">
            <TextBlock Text="Home Page" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="52" Foreground="White"/>
            
        </Border>
    </Grid>
</UserControl>
